{
  "redirects": [
    { "source": "/news/:slug", "destination": "/guides/:slug", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(?:png|jpg|jpeg|gif|webp|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
